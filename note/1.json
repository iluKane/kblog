        { "indent": "2", "value": "padding: 10px;" },
        { "indent": "2", "value": "border-radius: 10px;" },
        { "indent": "2", "value": "background-color: #fff;" },
        { "indent": "0", "value": "}" },
        { "indent": "0", "value": ".values {" },
        { "indent": "2", "value": "display: flex;" },
        { "indent": "2", "value": "justify-content: space-between;" },
        { "indent": "2", "value": "padding: 0 2px;" },
        { "indent": "0", "value": "}" },
        { "indent": "0", "value": "iframe {" },
        { "indent": "2", "value": "height: 600px" },
        { "indent": "2", "value": "border: 1px solid #00f;" },
        { "indent": "0", "value": "}" }
      ],
      "js": [
        {
          "indent": "0",
          "value": "document.addEventListener('DOMContentLoaded', () => {"
        },
        {
          "indent": "2",
          "value": "const img = document.getElementById('image');"
        },
        {
          "indent": "2",
          "value": "const controls = document.querySelectorAll('.controls input[type=range]');"
        },
        {
          "indent": "2",
          "value": "const fileInput = document.getElementById('upload');"
        },
        { "indent": "2", "value": "controls.forEach((control) => {" },
        {
          "indent": "4",
          "value": "control.addEventListener('input', updateFilters);"
        },
        { "indent": "2", "value": "});" },
        {
          "indent": "2",
          "value": "fileInput.addEventListener('change', (event) => {"
        },
        { "indent": "4", "value": "const file = event.target.files[0];" },
        { "indent": "4", "value": "if (file) {" },
        { "indent": "6", "value": "const reader = new FileReader();" },
        { "indent": "6", "value": "reader.onload = (e) => {" },
        { "indent": "8", "value": "img.src = e.target.result;" },
        { "indent": "6", "value": "};" },
        { "indent": "6", "value": "reader.readAsDataURL(file);" },
        { "indent": "4", "value": "}" },
        { "indent": "2", "value": "});" },
        { "indent": "2", "value": "function updateFilters() {" },
        {
          "indent": "4",
          "value": "const blur = document.getElementById('blur').value;"
        },
        {
          "indent": "4",
          "value": "const brightness = document.getElementById('brightness').value;"
        },
        {
          "indent": "4",
          "value": "const contrast = document.getElementById('contrast').value;"
        },
        {
          "indent": "4",
          "value": "const grayscale = document.getElementById('grayscale').value;"
        },
        {
          "indent": "4",
          "value": "const hueRotate = document.getElementById('hue-rotate').value;"
        },
        {
          "indent": "4",
          "value": "const invert = document.getElementById('invert').value;"
        },
        {
          "indent": "4",
          "value": "const opacity = document.getElementById('opacity').value;"
        },
        {
          "indent": "4",
          "value": "const saturate = document.getElementById('saturate').value;"
        },
        {
          "indent": "4",
          "value": "const sepia = document.getElementById('sepia').value;"
        },
        {
          "indent": "4",
          "value": "const dropShadow = document.getElementById('drop-shadow').value;"
        },
        {
          "indent": "4",
          "value": "const dropShadowX = document.getElementById('drop-shadowX').value;"
        },
        {
          "indent": "4",
          "value": "const dropShadowY = document.getElementById('drop-shadowY').value;"
        },
        { "indent": "4", "value": "img.style.filter = `" },
        { "indent": "6", "value": "blur(${blur}px)" },
        { "indent": "6", "value": "brightness(${brightness})" },
        { "indent": "6", "value": "contrast(${contrast})" },
        { "indent": "6", "value": "grayscale(${grayscale})" },
        { "indent": "6", "value": "hue-rotate(${hueRotate}deg)" },
        { "indent": "6", "value": "invert(${invert})" },
        { "indent": "6", "value": "opacity(${opacity})" },
        { "indent": "6", "value": "saturate(${saturate})" },
        { "indent": "6", "value": "sepia(${sepia})" },
        {
          "indent": "6",
          "value": "drop-shadow(${dropShadowX}px ${dropShadowY}px ${dropShadow}px #000)"
        },
        { "indent": "4", "value": "`;" },
        {
          "indent": "4",
          "value": "document.getElementById('blur-value').textContent = blur;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('brightness-value').textContent = brightness;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('contrast-value').textContent = contrast;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('grayscale-value').textContent = grayscale;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('hue-rotate-value').textContent = hueRotate;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('invert-value').textContent = invert;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('opacity-value').textContent = opacity;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('saturate-value').textContent = saturate;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('sepia-value').textContent = sepia;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('drop-shadow-value').textContent = dropShadow;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('drop-shadowX-value').textContent = dropShadowX;"
        },
        {
          "indent": "4",
          "value": "document.getElementById('drop-shadowY-value').textContent = dropShadowY;"
        },
        { "indent": "2", "value": "}" },
        { "indent": "2", "value": "updateFilters();" },
        { "indent": "0", "value": "});" },
        { "indent": "0", "value": "" }
      ]
    }
  ]
}
